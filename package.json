  "files": [
    "index.js",
    "index.d.ts",
    "cheap-ruler.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mapbox/cheap-ruler.git"
  },
  "keywords": [
    "geospatial",
    "measurement",
    "approximation",
    "distance"
